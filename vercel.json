{
    "version": 2,
    "builds": [
      { "src": "main.py", "use": "@now/python" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/docs/static/$1" },
      { "src": "/(.*)", "dest": "/main.py" }
    ],
    "includeFiles": ["main.py", "requirements.txt", "docs/index.html", "docs/static/*", "aiohttp*"]
  }
  